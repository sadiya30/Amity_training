#opening a file and choosing only those instances where difference of col 2 and col 1 is 164

import re
fo = open("test_chr13-22.tsv","r", encoding ="utf8", errors="ignore")
while True:
   line = fo.readline()
   if not line:
      break
   
   res = re.split('\t', line)
   b=int(res[2])-int(res[1])
   if(b == 164):
       print(res[0]," ",res[1]," ",res[2]," ",b)
